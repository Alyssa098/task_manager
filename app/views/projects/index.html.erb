<div id="header"><h1>Projects Index</h1></div>
<div id="newsub">

<p>
  <% if current_user %>

      <h4><%= current_user.name.capitalize %>'s Projects</h4>
  
<% @projects.each do |project|%>


	<p><div class="draggable"><%= link_to(project.name, project_path(project.name)) %></p>

<%end%>

<% else %>

  <p><%= "No Projects yet"%></p>
  
<% end %>
</div>
</p>
</div>
<p>
<%= link_to("New Project", new_project_path) %></p>

</div>